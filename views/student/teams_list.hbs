<div class="col-md-12 col-sm-12 col-xs-12">
  <div class="x_panel">

    <div class="x_title">
      <h2>Browse all teams<small>Show the team profile.</small></h2>

      <ul class="nav navbar-right panel_toolbox">
        <li><a href="/student/team/profile/edit">Edit</a></li>
        </li>
      </ul>
      <div class="clearfix"></div>
    </div>
    <div class="x_content">
      <table id="datatable-responsive" class="table table-striped table-bordered dt-responsive nowrap" cellspacing="0" width="100%">
        <thead>
          <tr>
            <th>Team Name</th>
            <th>Project Name</th>
            <th>Project Description</th>
          </tr>
        </thead>
        <tbody>
          {{#each teams}}
          <tr>
            <td><a href='/student/team/view/{{id}}'>{{teamName}}</a></td>
            <td>{{projectName}}</td>
            <td>{{projectDescription}}</td>
          </tr>
          {{/each}}
        </tbody>

    </div>
  </div>
</div>
